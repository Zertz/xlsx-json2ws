{
  "name": "json2xlsx-ws",
  "version": "1.0.1",
  "description": "Convert JSON objects to a xlsx worksheet",
  "main": "dist/index.js",
  "module": "dist/xlsx-json2ws.esm.js",
  "typings": "dist/index.d.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "dependencies": {
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.9.1",
    "husky": "^4.3.8",
    "size-limit": "^4.9.1",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zertz/xlsx-json2ws.git"
  },
  "keywords": [
    "convert",
    "json",
    "object",
    "xlsx",
    "worksheet"
  ],
  "author": "Pier-Luc Gendreau",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zertz/xlsx-json2ws/issues"
  },
  "homepage": "https://github.com/Zertz/xlsx-json2ws#readme",
  "files": [
    "LICENSE",
    "dist",
    "src"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "size-limit": [
    {
      "path": "dist/xlsx-json2ws.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/xlsx-json2ws.esm.js",
      "limit": "10 KB"
    }
  ]
}
